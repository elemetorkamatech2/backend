{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true,
        "node":true,
        "react":true
    },
    "extends": "airbnb-base",
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "max-len": ["error", { "code": 120 }],
        "linebreak-style": ["error", "unix"],
        "object-curly-newline": ["error", { "multiline": true, "consistent": true }]
    }
}
